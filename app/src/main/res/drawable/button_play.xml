<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true" >
    <shape>
        <solid
            android:color="@color/DarkGreen" />
        <stroke
            android:width="1dp"
            android:color="#EDC882"  />
        <corners
            android:radius="200dp" />
        <padding
            android:left="10dp"
            android:top="10dp"
            android:right="10dp"
            android:bottom="10dp" />
    </shape>
</item>

<item>
    <shape>
        <solid
            android:color="@color/Peach" />
        <stroke
            android:width="2dp"
            android:color="#EDC882" />
        <corners
            android:radius="200dp" />
        <padding
            android:left="10dp"
            android:top="10dp"
            android:right="10dp"
            android:bottom="10dp" />
    </shape>
</item>
</selector>
